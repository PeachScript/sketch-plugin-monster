{
  "name": "sketch-plugin-monster",
  "version": "0.4.0",
  "description": "A Sketch plugin for managing all plugin shortcuts",
  "scripts": {
    "start": "npm run start:webview & npm run start:sketch",
    "start:sketch": "skpm-build --watch",
    "start:webview": "webpack-dev-server --config ./webpack.webview.config.js -d --progress",
    "watch": "skpm-build --watch",
    "build": "rm -rf \"Sketch Plugin Monster.sketchplugin\" && npm run build:sketch && npm run build:webview",
    "build:sketch": "NODE_ENV=production skpm-build",
    "build:webview": "NODE_ENV=production webpack -p --progress --hide-modules --config ./webpack.webview.config.js",
    "release": "bash ./scripts/release.sh",
    "postinstall": "rm -rf \"Sketch Plugin Monster.sketchplugin\" && mkdir \"Sketch Plugin Monster.sketchplugin\" && skpm-link"
  },
  "author": "PeachScript <scdzwyxst@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeachScript/sketch-plugin-monster.git"
  },
  "keywords": [
    "sketch-plugin",
    "shortcuts"
  ],
  "engines": {
    "sketch": ">=3.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PeachScript/sketch-plugin-monster/issues"
  },
  "homepage": "https://github.com/PeachScript/sketch-plugin-monster",
  "skpm": {
    "name": "Sketch Plugin Monster",
    "manifest": "src/manifest.json",
    "main": "Sketch Plugin Monster.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "dependencies": {
    "@skpm/fs": "^0.1.2",
    "@skpm/os": "^0.1.1",
    "@skpm/path": "^0.1.0",
    "normalize.css": "^8.0.0",
    "sketch-module-web-view": "^1.1.3",
    "vue": "^2.5.16",
    "vue-i18n": "^7.6.0"
  },
  "devDependencies": {
    "@skpm/builder": "^0.4.3",
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-vue": "^4.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.6",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.2",
    "sass-loader": "^6.0.7",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url-loader": "^1.0.1",
    "vue-loader": "^14.2.1",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
