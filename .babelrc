{
  "env": {
    "webview": {
      "presets": [
        [
          "env",
          {
            "modules": false,
            "targets": {
              "browsers": [
                "> 1%",
                "last 1 versions",
                "last 4 Android versions",
                "last 3 iOS versions"
              ]
            }
          }
        ],
        "stage-2"
      ],
      "plugins": [
        "transform-runtime"
      ]
    },
    "test": {
      "presets": [
        ["env", { "targets": { "node": "current" }}]
      ]
    }
  }
}
